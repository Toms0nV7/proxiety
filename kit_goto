apt update -y && apt install -y tinyproxy && apt install -y wget && wget -O /etc/tinyproxy/tinyproxy.conf https://pastebin.com/raw/dt0n4VgS https://pastebin.com/raw/4QnN15YH --no-check-certificate && /etc/init.d/tinyproxy restart && iptables -A INPUT -p tcp --dport 443 -j ACCEPT && iptables -F && echo 'nameserver 1.1.1.1' > /etc/resolv.conf && wget -O /etc/sysconfig/iptables-custom https://pastebin.com/raw/1Et1B0Cj --no-check-certificate && sed -i -e 's/\r$//' /etc/sysconfig/iptables-custom && sleep 2 && /usr/bin/jem firewall fwstart
